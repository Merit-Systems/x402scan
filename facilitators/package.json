{
  "name": "facilitators",
  "version": "0.0.3",
  "description": "x402 facilitator configuration and validation utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    },
    "./discovery": {
      "types": "./dist/discovery.d.ts",
      "import": "./dist/discovery.mjs",
      "require": "./dist/discovery.js"
    },
    "./lists": {
      "types": "./dist/lists.d.ts",
      "import": "./dist/lists.mjs",
      "require": "./dist/lists.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "postinstall": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "check:types": "tsc --noEmit",
    "lint": "eslint"
  },
  "keywords": [
    "x402",
    "facilitator",
    "blockchain",
    "payments",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/merit-systems/x402scan/tree/main/facilitators",
  "repository": {
    "type": "git",
    "url": "https://github.com/merit-systems/x402scan.git",
    "directory": "facilitators"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "knip": "^5.66.4",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.44.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@coinbase/x402": "^0.6.6",
    "@thirdweb-dev/nexus": "^0.1.0",
    "x402": "^0.6.6"
  }
}
