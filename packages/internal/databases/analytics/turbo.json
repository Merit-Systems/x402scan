{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "description": "Build analytics database package",
      "env": ["ANALYTICS_CLICKHOUSE_*"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "description": "Start analytics database package in development mode",
      "env": ["ANALYTICS_CLICKHOUSE_*"],
      "persistent": true
    }
  }
}
