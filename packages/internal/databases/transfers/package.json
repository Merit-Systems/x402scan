{
  "name": "@x402scan/transfers-db",
  "version": "1.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:format": "prisma format",
    "dev": "tsup src/* --watch",
    "lint": "eslint . --max-warnings 0",
    "types:check": "tsc --noEmit",
    "studio": "prisma studio",
    "knip": "pnpm -w knip --workspace ./packages/internal/databases/transfers",
    "format": "pnpm -w format:dir ./packages/internal/databases/transfers",
    "format:check": "pnpm -w format:check:dir ./packages/internal/databases/transfers"
  },
  "dependencies": {
    "@neondatabase/serverless": "catalog:prisma",
    "@prisma/adapter-neon": "catalog:prisma",
    "@prisma/client": "catalog:prisma",
    "dotenv": "catalog:env",
    "ws": "catalog:prisma"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/ws": "catalog:prisma",
    "@x402scan/eslint-config": "workspace:*",
    "@x402scan/typescript-config": "workspace:*",
    "eslint": "catalog:",
    "prisma": "catalog:prisma",
    "rimraf": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
