{
  "name": "facilitators",
  "version": "0.0.10",
  "description": "x402 facilitator configuration and validation utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "check:types": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "knip": "pnpm -w knip --workspace ./packages/external/facilitators",
    "format": "pnpm -w format-dir ./packages/external/facilitators",
    "check:format": "pnpm -w check:format-dir ./packages/external/facilitators"
  },
  "keywords": [
    "x402",
    "facilitator",
    "blockchain",
    "payments",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/merit-systems/x402scan/tree/main/facilitators",
  "repository": {
    "type": "git",
    "url": "https://github.com/merit-systems/x402scan.git",
    "directory": "facilitators"
  },
  "devDependencies": {
    "@x402scan/eslint-config": "workspace:*",
    "@x402scan/typescript-config": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@coinbase/x402": "^0.6.6",
    "x402": "^0.6.6"
  }
}
