{
  "name": "facilitators",
  "version": "0.0.9",
  "description": "x402 facilitator configuration and validation utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "postinstall": "pnpm run build",
    "prepublishOnly": "pnpm run build",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "knip": "knip"
  },
  "keywords": [
    "x402",
    "facilitator",
    "blockchain",
    "payments",
    "web3"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/merit-systems/x402scan/tree/main/facilitators",
  "repository": {
    "type": "git",
    "url": "https://github.com/merit-systems/x402scan.git",
    "directory": "facilitators"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "^9.38.0",
    "knip": "^5.66.4",
    "prettier": "^3.6.2",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@coinbase/x402": "^0.6.6",
    "x402": "^0.6.6"
  }
}
