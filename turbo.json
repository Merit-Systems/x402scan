{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"],
      "env": [
        "BASE_RPC_URL",
        "CRON_SECRET",
        "DISCORD_WEBHOOK_URL",
        "GITHUB_TOKEN",
        "NODE_ENV",
        "NEXT_RUNTIME"
      ]
    },
    "format": {
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "VERCEL_ENV",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_BRANCH_URL"
  ]
}
