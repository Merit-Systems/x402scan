    "dotenv": "catalog:env",
    "facilitators": "workspace:*",
    "hono": "catalog:hono",
    "x402-hono": "catalog:x402"
  },
  "devDependencies": {
    "@x402scan/eslint-config": "workspace:*",
    "@x402scan/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
