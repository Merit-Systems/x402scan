{
  "name": "hono-server-example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check:types": "tsc --noEmit",
    "knip": "pnpm -w knip --workspace ./examples/servers/hono",
