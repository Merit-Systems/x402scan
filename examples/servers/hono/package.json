{
  "name": "hono-server-example",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx index.ts",
    "check-format": "prettier -c .prettierrc --check \"**/*.{ts,js,cjs,json,md}\"",
    "format": "prettier -c .prettierrc --write \"**/*.{ts,js,cjs,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check-types": "tsc --noEmit",
    "knip": "knip"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.8",
    "dotenv": "^16.4.7",
    "facilitators": "workspace:*",
    "hono": "^4.7.1",
    "x402-hono": "^0.7.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.9.2",
    "eslint": "^9.24.0",
    "knip": "^5.66.2",
    "prettier": "3.5.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
