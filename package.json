{
  "name": "x402scan-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev:scan": "turbo run dev --filter=x402scan",
    "build:scan": "turbo run build --filter=x402scan",
    "build:proxy": "turbo run build --filter=x402proxy",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write --cache .",
    "check": "turbo run check:types lint check:knip check:format",
    "check:format": "prettier --check .",
    "check:types": "turbo run check:types",
    "check:knip": "knip"
  },
  "devDependencies": {
    "knip": "^5.69.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.1",
    "typescript": "^5"
  },
  "pnpm": {
    "overrides": {
      "@auth/core": "0.40.0"
    },
    "onlyBuiltDependencies": [
      "@coinbase/x402"
    ]
  }
}
