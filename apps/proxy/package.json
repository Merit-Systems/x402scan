{
  "name": "x402proxy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "check:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "knip": "pnpm -w knip --workspace ./apps/proxy",
    "format": "pnpm -w format-dir ./apps/proxy",
    "check:format": "pnpm -w check:format-dir ./apps/proxy"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@x402scan/analytics-db": "workspace:*",
    "dotenv": "catalog:env",
    "hono": "^4.6.14"
  },
  "devDependencies": {
    "@x402scan/eslint-config": "workspace:*",
    "@x402scan/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
